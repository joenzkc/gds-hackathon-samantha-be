    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "npm i && tsc",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migrate": "npm run typeorm migration:run -- -d src/datasource.ts",
    "migrate-build": "npm run typeorm migration:run -- -d build/datasource.js",
    "start": "npm run migrate-build && ts-node build/index.js",
    "revert": "npm run typeorm migration:revert -- -d src/datasource.ts",
    "generate": "npm run typeorm migration:generate -- -d src/datasource.ts",
    "tsc": "tsc --esModuleInterop"
